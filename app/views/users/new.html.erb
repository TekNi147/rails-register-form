<h1>Create User</h1>
<%= form_with(model: @user, url: register_path) do |form| %>
  <div>
    <%= form.label :firstName %>
    <%= form.text_field :firstName %>
  </div>
  <div>
    <%= form.label :lastName %>
    <%= form.text_field :lastName %>
  </div>
  <div>
    <%= form.label :birthDay %>
    <%= form.date_field :birthDay %>
  </div>
</div>
<div>
  <%= form.label :gender, "Select Gender" %><br>
  <%= form.radio_button :gender, "Male" %>
  <%= form.label :gender, "Male", value: "Male" %><br>
  <%= form.radio_button :gender, "Female" %>
  <%= form.label :gender, "Female", value: "Female" %><br>
</div>
<div>
  <%= form.label :email %>
  <%= form.email_field :email %>
</div>
<div>
  <%= form.label :phone %>
  <%= form.text_field :phone %>
</div>
<div>
  <%= form.label :subject %>
  <%= form.select :subject, options_for_select([["Thailand", "TH"], ["United States", "US"], ["United Kingdom", "UK"]]) %>
</div>
<%= form.submit %>
</div>
<% end %>
<%= link_to 'View User', users_path %>
